<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scrollview_doc_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:layout_marginBottom="@dimen/margin_45dp">

        <RelativeLayout
            android:id="@+id/relativelayout_cardview"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_below="@id/scrollview_doc_home"
            android:layout_centerHorizontal="true"
            android:scrollbars="vertical">

            <android.support.v7.widget.CardView
                android:id="@+id/cardview_doc_home_listofappointment"
                android:layout_width="300dp"
                android:layout_height="230dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/margin_30dp"
                android:layout_marginRight="@dimen/margin_30dp"
                android:layout_marginTop="@dimen/margin_10dp"
                android:foreground="?attr/selectableItemBackgroundBorderless"
                app:cardCornerRadius="@dimen/radius_10dp"
                app:cardElevation="@dimen/width_24dp"
                app:cardPreventCornerOverlap="true">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_20dp"
                    android:paddingLeft="@dimen/padding_10dp"
                    android:paddingRight="@dimen/padding_10dp">

                    <ImageView
                        android:id="@+id/imageview_doc_listofappointment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="@dimen/margin_20dp"
                        android:layout_marginRight="@dimen/margin_20dp"
                        android:layout_marginTop="@dimen/margin_10dp"
                        android:src="@drawable/listofappointmentcopy" />


                    <TextView
                        android:id="@+id/textview_doc_listofappointment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageview_doc_listofappointment"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_5dp"
                        android:text="@string/list_of_appointments"
                        android:textSize="@dimen/font_18" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/cardview_doc_home_myschedule"
                android:layout_width="300dp"
                android:layout_height="230dp"
                android:layout_below="@id/cardview_doc_home_listofappointment"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/margin_30dp"
                android:layout_marginRight="@dimen/margin_30dp"
                android:layout_marginTop="@dimen/margin_10dp"
                android:foreground="?attr/selectableItemBackgroundBorderless"
                app:cardCornerRadius="@dimen/radius_10dp"
                app:cardElevation="@dimen/width_24dp"
                app:cardPreventCornerOverlap="true">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_20dp"
                    android:paddingLeft="@dimen/padding_10dp"
                    android:paddingRight="@dimen/padding_10dp">

                    <ImageView
                        android:id="@+id/imageview_doc_myschedule"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_20dp"
                        android:layout_marginRight="@dimen/margin_20dp"
                        android:layout_marginTop="@dimen/margin_10dp"
                        android:src="@drawable/myschedulecopy" />


                    <TextView
                        android:id="@+id/textview_doc_myschedule"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/imageview_doc_myschedule"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/margin_5dp"
                        android:text="@string/my_schedule"
                        android:textSize="@dimen/font_18" />

                </RelativeLayout>

            </android.support.v7.widget.CardView>

        </RelativeLayout>
    </ScrollView>

